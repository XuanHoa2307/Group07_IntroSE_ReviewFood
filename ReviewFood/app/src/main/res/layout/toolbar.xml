<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.Toolbar
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/toolbar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#668AAAE5"
    android:minHeight="?attr/actionBarSize"
    android:theme="@style/ThemeOverlay.AppCompat.Dark">

    <ImageButton
        android:id="@+id/btn_menu"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/menu_toolbar"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fontFamily="@font/inter_medium"
        android:shadowColor="#FF04A2F1"
        android:shadowDx="1.5"
        android:shadowDy="1.3"
        android:shadowRadius="10"
        android:singleLine="false"
        android:text="ReviewFood  "
        android:textAlignment="viewEnd"
        android:textColor="#010FBA"
        android:textSize="34sp"
        tools:ignore="HardcodedText,RtlCompat,RtlHardcoded,TextContrastCheck" />

</androidx.appcompat.widget.Toolbar>